@model IEnumerable<PeopleEvents.Models.Person>
@{
    ViewBag.Title = "People List";
}

<div class="row">
	<h1>List of People</h1>

	<table>
		@foreach (var person in Model)
		{
			<tr>
				<td>
					@Html.ActionLink(person.Name, "View", "Person", new { id = person.ID }, null)
				</td>
			</tr>
		}
	</table>

</div>